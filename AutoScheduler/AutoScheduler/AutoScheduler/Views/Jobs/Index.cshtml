@model IEnumerable<AutoScheduler.Entities.Job>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        
        <th>
            @Html.DisplayNameFor(model => model.JobName)
        </th>
      
        <th>
            @Html.DisplayNameFor(model => model.Enabled)
        </th>
       
        <th>
            @Html.DisplayNameFor(model => model.cron)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.fetchXml)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.workflowName)
        </th>
       
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.JobName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Enabled)
        </td>
      
        <td>
            @Html.DisplayFor(modelItem => item.cron)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.fetchXml)
        </td>
       
        <td>
            @Html.DisplayFor(modelItem => item.workflowName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>